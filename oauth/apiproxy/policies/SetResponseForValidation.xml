<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="SetResponseForValidation">
    <DisplayName>SetValidTokenResponse</DisplayName>
    <FaultRules/>
    <Properties/>
    <Set>
        <Payload contentType="application/json" variablePrefix="%" variableSuffix="#">{"username":"%accesstoken.username#"}</Payload>
        <StatusCode>200</StatusCode>
        <ReasonPhrase>OK</ReasonPhrase>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo type="response" transport="http" createNew="true"/>
</AssignMessage>