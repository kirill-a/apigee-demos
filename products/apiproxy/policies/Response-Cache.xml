<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ResponseCache async="false" continueOnError="false" enabled="true" name="Response-Cache">
    <DisplayName>Response Cache</DisplayName>
    <Properties/>
    <CacheKey>
        <Prefix/>
        <KeyFragment ref="proxy.pathsuffix" type="string"/>
        <KeyFragment ref="request.queryparam.color"/>
        <KeyFragment ref="request.queryparam.productid"/>
        <KeyFragment ref="request.queryparam.storeid"/>
    </CacheKey>
    <UseAcceptHeader>true</UseAcceptHeader>
    <ExcludeErrorResponse>true</ExcludeErrorResponse>
    <CacheResource>ProductsCache</CacheResource>
    <Scope>Exclusive</Scope>
    <SkipCacheLookup/>
    <SkipCachePopulation/>
</ResponseCache>